- push( hoo_text_list_view )


%ul[_]
    - _.dataSrc.allItems.each do |item|
        -# Woah, whitespace is important! = must be butted to bracket. joy!
        - if( item['url']=='' )
            %li( class="smallListType" )= item['name']
        - else
            %li( class="smallListType" )= link_to( item['name'], item['url'] )
.clear
